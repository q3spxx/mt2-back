{
    "name": "mt2-server",
    "version": "1.0.0",
    "scripts": {
        "server": "node index.js",
        "start": "nodemon dist/index.js",
        "build": "npx babel src --extensions .ts --out-dir dist",
        "build:watch": "npx babel src --extensions .ts --out-dir dist --watch",
        "build:docker": "docker build -t mt2-back .",
        "docker:run": "docker run --name mt2-back -dp 8081:8081 mt2-back",
        "docker:start": "docker start mt2-back",
        "docker:stop": "docker stop mt2-back",
        "lint": "eslint --ext .ts ./src/",
        "lint:fix": "eslint --fix --ext .ts ./src/",
        "prettier": "prettier --write ./src/**/*.ts{,x}",
        "check-types": "tsc --noEmit"
    },
    "devDependencies": {
        "@babel/cli": "7.13.14",
        "@babel/core": "7.13.15",
        "@babel/plugin-transform-runtime": "7.13.15",
        "@babel/plugin-transform-typescript": "7.13.0",
        "@babel/preset-env": "7.13.15",
        "@babel/preset-typescript": "7.13.0",
        "@types/cors": "2.8.10",
        "@types/express": "4.17.11",
        "@types/node": "12.20.10",
        "@types/pg": "7.14.11",
        "@typescript-eslint/eslint-plugin": "2.34.0",
        "@typescript-eslint/parser": "2.34.0",
        "babel-plugin-module-resolver": "4.1.0",
        "babel-plugin-transform-class-properties": "6.24.1",
        "eslint": "7.24.0",
        "eslint-config-airbnb-base": "14.2.1",
        "eslint-config-prettier": "8.2.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-prettier": "3.4.0",
        "nodemon": "2.0.7",
        "prettier": "2.2.1",
        "typescript": "4.2.4"
    },
    "dependencies": {
        "cors": "2.8.5",
        "express": "4.17.1",
        "express-joi-validation": "5.0.0",
        "joi": "17.4.0",
        "pg": "8.6.0",
        "pg-hstore": "2.3.3",
        "sequelize": "6.6.2"
    }
}
